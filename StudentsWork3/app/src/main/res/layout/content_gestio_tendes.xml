<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context=".View.GestioTendesActivity"
    tools:showIn="@layout/activity_gestio_tendes">

    <EditText
        android:id="@+id/edit_URL"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="8dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text=""
        app:layout_constraintBottom_toTopOf="@+id/boto_list"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView8" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="7dp"
        android:layout_marginEnd="10dp"
        android:layout_marginBottom="15dp"
        android:text="Adreça"
        app:layout_constraintBottom_toTopOf="@+id/boto_insert"
        app:layout_constraintEnd_toStartOf="@+id/edit_Adreça"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />

    <EditText
        android:id="@+id/edit_Adreça"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="10dp"
        android:layout_marginBottom="8dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text=""
        app:layout_constraintBottom_toTopOf="@+id/boto_insert"
        app:layout_constraintEnd_toStartOf="@+id/textView8"
        app:layout_constraintStart_toEndOf="@+id/textView3" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="2dp"
        android:layout_marginBottom="7dp"
        android:text="URL Foto"
        app:layout_constraintBottom_toBottomOf="@+id/edit_URL"
        app:layout_constraintEnd_toStartOf="@+id/edit_URL"
        app:layout_constraintStart_toEndOf="@+id/edit_Adreça" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="10dp"
        android:text="Telefon"
        app:layout_constraintBottom_toBottomOf="@+id/edit_Poblacio"
        app:layout_constraintEnd_toStartOf="@+id/edit_Telefon"
        app:layout_constraintStart_toEndOf="@+id/edit_Poblacio" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="4dp"
        android:layout_marginBottom="40dp"
        android:text="Poblacio"
        app:layout_constraintBottom_toTopOf="@+id/textView3"
        app:layout_constraintEnd_toStartOf="@+id/edit_Poblacio"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView6" />

    <EditText
        android:id="@+id/edit_Poblacio"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="9dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text=""
        app:layout_constraintBaseline_toBaselineOf="@+id/textView2"
        app:layout_constraintEnd_toStartOf="@+id/textView"
        app:layout_constraintStart_toEndOf="@+id/textView2" />

    <EditText
        android:id="@+id/edit_Telefon"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="16dp"
        android:ems="10"
        android:inputType="number|numberSigned|numberDecimal"
        android:numeric="integer|signed|decimal"
        android:text=""
        app:layout_constraintBaseline_toBaselineOf="@+id/textView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="7dp"
        android:layout_marginTop="31dp"
        android:layout_marginEnd="3dp"
        android:layout_marginBottom="30dp"
        android:text="Codi:"
        app:layout_constraintBottom_toTopOf="@+id/textView6"
        app:layout_constraintEnd_toStartOf="@+id/edit_codi"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/edit_codi"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="9dp"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toStartOf="@+id/textView5"
        app:layout_constraintStart_toEndOf="@+id/textView4"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="12dp"
        android:text="Nom:"
        app:layout_constraintBottom_toBottomOf="@+id/edit_codi"
        app:layout_constraintEnd_toStartOf="@+id/edit_nom"
        app:layout_constraintStart_toEndOf="@+id/edit_codi" />

    <EditText
        android:id="@+id/edit_nom"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintBaseline_toBaselineOf="@+id/textView5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView5" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="7dp"
        android:layout_marginEnd="12dp"
        android:layout_marginBottom="39dp"
        android:text="Lat"
        app:layout_constraintBottom_toTopOf="@+id/textView2"
        app:layout_constraintEnd_toStartOf="@+id/edit_lat"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <EditText
        android:id="@+id/edit_lat"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="18dp"
        android:ems="10"
        android:inputType="number|numberSigned|numberDecimal"
        android:numeric="integer|signed|decimal"
        android:text=""
        app:layout_constraintEnd_toStartOf="@+id/textView7"
        app:layout_constraintStart_toEndOf="@+id/textView6"
        app:layout_constraintTop_toBottomOf="@+id/edit_codi" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="9dp"
        android:text="Lon"
        app:layout_constraintBottom_toBottomOf="@+id/edit_lon"
        app:layout_constraintEnd_toStartOf="@+id/edit_lon"
        app:layout_constraintStart_toEndOf="@+id/edit_lat" />

    <EditText
        android:id="@+id/edit_lon"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="29dp"
        android:ems="10"
        android:inputType="number|numberSigned|numberDecimal"
        android:numeric="integer|signed|decimal"
        android:text=""
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView7"
        app:layout_constraintTop_toBottomOf="@+id/edit_nom" />

    <Button
        android:id="@+id/boto_insert"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="5dp"
        android:layout_marginEnd="4dp"
        android:layout_marginBottom="7dp"
        android:text="INS"
        app:layout_constraintBottom_toTopOf="@+id/text_res"
        app:layout_constraintEnd_toStartOf="@+id/boto_update"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />

    <Button
        android:id="@+id/boto_update"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="UPD"
        app:layout_constraintEnd_toStartOf="@+id/bodo_borrar"
        app:layout_constraintStart_toEndOf="@+id/boto_insert"
        app:layout_constraintTop_toTopOf="@+id/boto_insert" />

    <Button
        android:id="@+id/bodo_borrar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="3dp"
        android:text="DEL"
        app:layout_constraintBaseline_toBaselineOf="@+id/boto_update"
        app:layout_constraintEnd_toStartOf="@+id/boto_list"
        app:layout_constraintStart_toEndOf="@+id/boto_update" />

    <Button
        android:id="@+id/boto_list"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="5dp"
        android:text="LIST"
        app:layout_constraintBaseline_toBaselineOf="@+id/bodo_borrar"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/bodo_borrar" />

    <TextView
        android:id="@+id/text_res"
        android:layout_width="362dp"
        android:layout_height="0dp"
        android:layout_marginBottom="8dp"
        android:hint="Resultado"
        android:scrollbars="vertical"
        android:text=""
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/boto_update"
        app:layout_constraintStart_toEndOf="@+id/boto_update"
        app:layout_constraintTop_toBottomOf="@+id/boto_insert" />

</android.support.constraint.ConstraintLayout>